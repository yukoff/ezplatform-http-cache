{
    "name": "ezsystems/ezplatform-http-cache",
    "description": "HTTP cache handling for eZ Platform.",
    "type": "ezplatform-bundle",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "eZ Systems",
            "email": "dev-team@ez.no"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ezsystems/ezpublish-kernel": "^6.7|^7.0",
        "friendsofsymfony/http-cache-bundle": "^2.0@dev",
        "symfony/symfony": "^2.8|^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "matthiasnoback/symfony-dependency-injection-test": "*",
        "phpspec/phpspec": "^3.2",
        "memio/spec-gen": "^0.6",
        "php-http/guzzle6-adapter": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "EzSystems\\PlatformHttpCacheBundle\\": "src",
            "EzSystems\\PlatformHttpCacheBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev",
            "dev-fos_http_cache_2.0": "0.1.x-dev"
        }
    }
}
